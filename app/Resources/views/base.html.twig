        <script>
            $(document).ready(function () {
                window.scrollTo(0, 0);
                $('#login_submit').click(function (e) {
                    e.preventDefault();
                    if (usernamereq() === false && pswreq() === false) {
                        $('#loginerror').html("entrez vos coordonn√©es");
                    } else if (usernamereq() === false) {
                        $('#loginerror').html("nom d'utilisateur ou e-mail obligatoire");
                    } else if (pswreq() === false) {
                        $('#loginerror').html("mot de passe obligatoire");
                    } else {
                        $('#loginerror').html("");
                        $('#loginusername').parent().css("border", "none");
                        $('#loginpassword').parent().css("border", "none");
                        $.ajax({
                            type: $('#loginform').attr('method'),
                            url: '{{ path("fos_user_security_check") }}',
                            data: $('#loginform').serialize(),
                            dataType: "json",
                            success: function (data, status, object) {
                                if (!data.success) {
                                    $('#loginerror').html("nom utilisateur ou mot de passe invalide");
                                    $('#loginusername').parent().css("border", "solid 1px red");
                                    $('#loginpassword').parent().css("border", "solid 1px red");
                                } else
                                if ((location.href.match('/register')) || (location.href.match('/resetting/request')))
                                    window.location.href = "{{path('homepage')}}";
                                else
                                    location.reload();
                            },
                            error: function (data, status, object) {
                                console.log(data.message);

                            }
                        });
                    }
                });
            });
        </script>
