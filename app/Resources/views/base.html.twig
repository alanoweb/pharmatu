        <script>

            function usernamereq() {
                if ($('#loginusername').val() === "" || $('#loginusername').val() === null) {
                    $('#loginusername').parent().css("border", "solid 1px red");
                    return false;
                } else {
                    $('#loginusername').parent().css("border", "solid 1px green");
                    return true;
                }
            }
            function pswreq() {
                if ($('#loginpassword').val() === "" || $('#loginpassword').val() === null) {
                    $('#loginpassword').parent().css("border", "solid 1px red");
                    return false;
                } else {
                    $('#loginpassword').parent().css("border", "solid 1px green");
                    return true;
                }
            }
            $('#login-modal').on('hidden.bs.modal', function () {
                $('#loginerror').html("");
                $('#loginusername').parent().css("border", "none");
                $('#loginpassword').parent().css("border", "none");
            });
            $('#_register').click(function (e) {
                if (location.href.match('/register')) {
                    e.preventDefault();
                    $('#login-modal').modal('toggle');
                }
            });
            $('#forgotpwd').click(function (e) {
                if (location.href.match('/resetting/request')) {
                    e.preventDefault();
                    $('#login-modal').modal('toggle');
                }
            });

        </script>
